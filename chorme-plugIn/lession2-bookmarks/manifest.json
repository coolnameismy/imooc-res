{
  "manifest_version": 2,

  "name": "book marks search",
  "description": " ",
  "version": "0.0.1",
  "permissions": [
    "bookmarks"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
	"matches": ["<all_urls>"],
	"js": [
		"./scripts/contents_script.js"
	]
  }]
}